{
  "name": "bridit/framework",
  "description": "Brid-IT PHP Micro-Framework",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Gustavo Siqueira",
      "email": "gus@brid-it.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "bridit/bifrost-support": "^5.0",
    "guzzlehttp/psr7": "^1.8",
    "lcobucci/jwt": "^4.1",
    "php-di/slim-bridge": "^3.1",
    "ramsey/uuid": "^4.1",
    "slim/csrf": "^1.2",
    "slim/slim": "^4.0",
    "vlucas/phpdotenv": "^5.3",
    "monolog/monolog": "^2.2",
    "symfony/console": "^5.3",
    "symfony/process": "^5.3",
    "nyholm/psr7": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12.88"
  },
  "autoload": {
    "psr-4": {
      "Bridit\\Framework\\": "src/"
    },
    "files": [
      "src/Foundation/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
